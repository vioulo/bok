{
    "name": "bok",
    "description": "收集链接",
    "version": "1.0",
    "manifest_version": 3,
    "omnibox": { "keyword" : "bok" },
    "options_page": "./pages/options.html",
    "permissions": [
        "tabs",
        "activeTab",
        "declarativeContent",
        "bookmarks",
        "storage",
        "unlimitedStorage"
    ],
    "icons": {
        "16": "imgs/icons/16.png",
        "32": "imgs/icons/32.png",
        "48": "imgs/icons/48.png",
        "128": "imgs/icons/128.png"
    },
    "background": {
        "service_worker": "js/background.js"
    },
    "content_scripts": [
        {
            "matches": ["http://*/*", "https://*/*"],
            "js": [
                "js/jquery-3.3.1.min.js",
                "js/content.js"
            ],
            "css": ["css/main.css"]
        }
    ],
    "chrome_url_overrides" : {
        "bookmarks": "./pages/bookmarks.html"
    }
}